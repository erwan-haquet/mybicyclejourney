{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "~2.6",
        "doctrine/doctrine-migrations-bundle": "~3.2",
        "doctrine/orm": "~2.12",
        "symfony/console": "~6.0.0",
        "symfony/doctrine-messenger": "~6.0.0",
        "symfony/dotenv": "~6.0.0",
        "symfony/flex": "~2.1",
        "symfony/form": "~6.0.0",
        "symfony/framework-bundle": "~6.0.0",
        "symfony/messenger": "~6.0.0",
        "symfony/proxy-manager-bridge": "~6.0.0",
        "symfony/runtime": "~6.0.0",
        "symfony/twig-bundle": "~6.0.0",
        "symfony/validator": "~6.0.0",
        "symfony/webpack-encore-bundle": "~1.14",
        "symfony/yaml": "~6.0.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Library\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "compile": [
            "php bin/console doctrine:migrations:migrate",
            "yarn build"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "~6.0.0"
        }
    }
}
